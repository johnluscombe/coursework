<h1>New Church</h1><br>

<%= render 'shared/errors', object: @church %>

<%= form_for @church do |form| %>
<%= render partial: 'fields', locals: { f: form } %>

<%= form.submit 'Create new church', class: 'btn btn-success' %>
<%= link_to 'Cancel', churches_path, class: 'btn btn-danger' %>

<% end %>